{
  "version": 2,
  "builds": [
    {
      "src": "server/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "serverless": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "server.settings",
    "SECRET_KEY": "@secret_key",
    "DATABASE_URL": "@database_url"
  }
}
